<h1>New Movie</h1>
<%= form_with model: @movie do |form|%>

    <p>
        <%= form.label :title%>
        <%= form.text_field :title%>
    </p>

    <p>
        <%= form.label :genra%>
        <%= form.text_field :genra%>
    </p>

    <p>
        <%= form.label :runtime%>
        <%= form.text_field :runtime%>
    </p>

    <p>
        <%= form.label :release_date%>
        <%= form.text_field :release_date%>
    </p>

    <p>
        <%= form.label :title%>
        <%= form.text_field :title%>
    </p>

    <%= form.submit "Save Movie"%>


<%end%>
